// Animation in the page change
.page_transition-forward-enter-active
.page_transition-forward-leave-active
.page_transition-backward-enter-active
.page_transition-backward-leave-active
  transition transform .4s, opacity .4s

.page_transition-forward-enter-active
.page_transition-backward-enter-active
  transition-delay .4s

.page_transition-enter-to
.page_transition-leave
  opacity 1
  transform translateY(0)

.page_transition-forward-enter
  opacity 0
  transform translateY(10px)

.page_transition-forward-leave-to
  opacity 0
  transform translateY(-10px)

.page_transition-backward-enter
  opacity 0
  transform translateY(-10px)

.page_transition-backward-leave-to
  opacity 0
  transform translateY(10px)

// Animation in the page change

.link
  position relative
  display inline-block
  @extend .ct-primary
  letter-spacing 2px
  text-transform uppercase
  &::after
    position absolute
    content ''
    @extend .bg-deep
    transition width .3s
  &.center_marker
    &::after
      bottom 0
      left 50%
      transform translate(-50%, 5px)
      width 0
      height 1px
    &.router-link-active
    &:hover
      &::after
        width 20px
  &.left_marker
    transition padding .3s
    &::after
      top 50%
      transform translateY(-50%)
      left 0
      width 0
      height 1px
    &.router-link-active
    &:hover
      padding-left 20px
      &::after
        width 10px
  &[class^="icon-"]
  &[class*=" icon-"]
    text-transform none
    &::before
      display inline-block
      width 30px
      height @width
      line-height @height
      font-size 16px
      margin-right 10px
      @extend .bg-deep
      @extend .ct-light
      border-radius 2em
      text-align center
      vertical-align -3px
.after-btn
  &::before
    //position absolute
    content ''
    background url("../assets/img/icons/ico-next.svg") no-repeat
    padding: 10px 10px;
    top: 25%;
    right: -20%;
    margin-left 110px
    vertical-align middle
  &:hover
    &:before
      animation-name moveRight_1
      animation-iteration-count infinite
      animation-duration 2.5s
      animation-timing-function ease-in-out

@keyframes moveRight_1
  0%
    transform scale3d(1)
  50%
    transform scale3d(1.2, 1.2, 1.4);
  100%
    transform scale3d(1)
.button
    position relative
    display inline-grid
    padding 10px 45px
    background transparent
    font-size 12px
    height 40px

    line-height @height
    letter-spacing 1px
    text-transform uppercase
    cursor pointer
    border none
    transition color .3s, background-color .3s, border-color .3s
  &.submit
      &::after
        position absolute
        top 50%
        left @top
        transform translate(-50%, -50%)
        content attr(data-text)
        white-space nowrap
      &.loading
      &.error
