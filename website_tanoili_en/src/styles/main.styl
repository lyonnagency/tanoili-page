[data-aos="new-animation"] {
  opacity: 0;
  transition-property: transform, opacity;
  &.aos-animate {
    opacity: 1;
  }
  transform: translateX(-10px);
  &.aos-animate {
    transform: translateX(0);
  }
}

[data-aos="animation-left"] {
  opacity 0
  transition-property: transform, opacity;
  transform: translateX(10px);
  &.aos-animate {
    transform: translateX(0);
    opacity: 1;
  }
}

[data-aos="just-fade"] {
  opacity: 0;
  transition-property:  opacity;
  &.aos-animate {
    opacity: 1;
  }
}

[data-aos="up-animation"] {
  opacity: 0;
  transition-property: transform, opacity;
  &.aos-animate {
    opacity: 1;
  }
  transform: translateY(-10px);
  &.aos-animate {
    transform: translateY(0);
  }
}
[data-aos="down-animation"] {
  opacity: 0;
  transition-property: transform, opacity;
  &.aos-animate {
    opacity: 1;
  }
  transform: translateY(10px);
  &.aos-animate {
    transform: translateY(0);
  }
}

